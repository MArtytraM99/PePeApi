{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "UseHttps": true,
  "ConnectionStrings": {
    "mongoDB": "mongodb://127.0.0.1:27017/",
    "pepeDbName": "pepedb",
    "pepeMenuCollectionName": "menu"
  },
  "HealthCheck": {
    "StatusUri": "/healthz",
